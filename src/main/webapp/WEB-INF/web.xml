<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>sgp</display-name>
	<servlet>
		<servlet-name>ListerCollaborateursController</servlet-name>
		<servlet-class>dev.sgp.web.ListerCollaborateursController
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>EditerCollaborateursController</servlet-name>
		<servlet-class>dev.sgp.web.EditerCollaborateursController
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>NouveauCollaborateurController</servlet-name>
		<servlet-class>dev.sgp.web.NouveauCollaborateurController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListerCollaborateursController</servlet-name>
		<url-pattern>/collaborateurs/lister</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>EditerCollaborateursController</servlet-name>
		<url-pattern>/collaborateur/editer</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>NouveauCollaborateurController</servlet-name>
		<url-pattern>/collaborateurs/nouveau</url-pattern>
	</servlet-mapping>

</web-app>